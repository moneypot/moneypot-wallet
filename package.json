{
  "name": "moneypot",
  "version": "0.0.1",
  "description": "moneypot wallet",
  "scripts": {
    "dev": "webpack-dev-server",
    "schema": "ts-node src/wallet/generate-schema.ts > src/wallet/schema.ts && prettier --single-quote --trailing-comma es5 --print-width 160 --write src/wallet/schema.ts",
    "build": "webpack --config webpack.config.js",
    "build-cdn": "NODE_ENV=production USE_CDN=true webpack --config webpack.config.js",
    "prettier": "prettier --single-quote --trailing-comma es5 --print-width 160 --write \"src/**/*.{ts,tsx,scss}\"",
    "publish": "npm run build-cdn && cp -r dist/* ../moneypot-wallet-dist && cd ../moneypot-wallet-dist/ && git add -A && git commit -a -S -m \"Add built files\" && git pull --rebase && git push"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-table": "^7.0.25",
    "@types/reactstrap": "^8.7.2",
    "@types/react-transition-group": "^4.4.0",
    "bootstrap": "^4.5.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "^7.0.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "reactstrap": "^8.7.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-subresource-integrity": "^1.5.2",
    "worker-loader": "^3.0.6"
  },
  "dependencies": {
    "@the-/ui-qr": "^16.0.0",
    "deep-equal": "^2.0.5",
    "idb": "^5.0.8",
    "javascript-time-ago": "^2.3.3",
    "moneypot-lib": "git+https://github.com/moneypot/moneypot-lib.git",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-qr-reader": "^2.2.1",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.2",
    "react-toastify": "^6.2.0",
    "react-transition-group": "^4.4.1"
  },
  "browser": {
    "./dist/bip39/pbkdf2-node.js": "./dist/bip39/pbkdf2-browser.js"
  }
}
